package stringmethods;

public class LabFive {
	public static void main (String[]args) {

		System.out.println(methods.capatalize("jAvA is COOL"));
		System.out.println(methods.wheresWaldo("Where did waldo go?"));
		System.out.println(methods.firstThingsFirst("double", "cat"));
		System.out.println(methods.reverse("Noah"));
		methods.soLong("YES","NO");
		System.out.println(methods.afterMath("This is a phrase wow"));
		methods.letterize("Java");	
	}
	
	
}

